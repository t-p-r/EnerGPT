{
    "name": "EnerGPT",
    "description": "nwhack25 project",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "sandbox": {
        "pages": [
            "index.html"
        ]
    },
    "content_security_policy": {
        "sandbox": "sandbox allow-scripts; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}